SELECT *
--    p0. "id",
--    p0. "amount",
--    p0. "charged_at",
--    p0. "description",
--    p0. "status",
--    p0. "customer_id",
--    p0. "inserted_at",
--    p0. "updated_at"
FROM
    "payments" AS p0
    INNER JOIN "customers" AS c1 ON c1. "id" = p0. "customer_id"
    WHERE (((p0. "id" = 11)
            AND ((c1. "name" < 'Charlie')
                AND true))
        OR (((p0. "id" < 11)
                AND true)
            AND true))
ORDER BY
    c1. "name" DESC,
    p0. "id" DESC
